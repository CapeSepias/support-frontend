{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-flow",
    [ "@babel/preset-env", {
      "modules": false,
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-exponentiation-operator",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true }],
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"]
    }
  }
}
